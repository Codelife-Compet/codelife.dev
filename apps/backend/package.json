{
  "name": "@codelife/backend",
  "version": "0.0.0",
  "private": true,
  "description": "backend para @codelife",
  "main": "./dist/core/index.js",
  "module": "./dist/core/index.mjs",
  "types": "./dist/core/index.d.ts",
  "scripts": {
    "build": "tsup src --out-dir dist",
    "start": "node build/core/index.js",
    "dev": "tsx watch src/core/index.ts",
    "docker:start": "docker compose up -d",
    "docker:stop": "docker compose stop",
    "prisma:migrate:dev": "npx prisma migrate dev",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-web",
    "test:install-prisma-environment": "npm link vitest-environment-web",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "pretest:e2e:watch": "npm-run-all test:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest run --dir src/tests/controllers",
    "test:e2e:watch": "vitest --dir src/tests/controllers"
  },
  "keywords": [],
  "author": "PedroRonzani18",
  "license": "ISC",
  "devDependencies": {
    "@codelife-ui/eslint-config": "*",
    "@codelife-ui/ts-config": "*",
    "@faker-js/faker": "^8.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.10.3",
    "@types/supertest": "^2.0.16",
    "@vitest/coverage-v8": "^0.34.1",
    "@vitest/ui": "^0.34.1",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.1.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsup": "^7.1.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.1"
  },
  "dependencies": {
    "@fastify/cookie": "^9.1.0",
    "@fastify/jwt": "^7.2.1",
    "@prisma/client": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "fastify": "^4.21.0",
    "glob": "^10.3.10",
    "googleapis": "^129.0.0",
    "python-shell": "^5.0.0",
    "zod": "^3.21.4"
  }
}
