{
  "name": "codelife-2.0",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "pages/**/*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "git add"
    ],
    "components/**/*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "git add"
    ],
    "styles/**/*.{css,scss,sass}": [
      "npm run lint:fix",
      "git add"
    ],
    "**/*.{js,jsx,ts,tsx,json,css,scss,sass,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@types/node": "18.15.10",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "axios": "^1.3.4",
    "eslint": "8.37.0",
    "eslint-config-next": "13.2.4",
    "i18next": "^22.4.14",
    "next": "13.2.4",
    "next-i18next": "^13.2.2",
    "nookies": "^2.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.2.0",
    "react-icons": "^4.8.0",
    "typescript": "5.0.3",
    "uuid": "^9.0.0",
    "@codelife-ui/react": "*",
    "@codelife-ui/tokens": "*"
  },
  "devDependencies": {
    "@codelife-ui/eslint-config": "*",
    "@codelife-ui/ts-config": "*",
    "@types/cookie": "^0.5.1",
    "@types/uuid": "^9.0.1",
    "autoprefixer": "^10.4.14",
    "eslint-plugin-tailwindcss": "^3.10.3",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.2.7"
  }
}
